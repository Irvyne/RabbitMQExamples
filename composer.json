{
    "name": "irvyne/rabbitmq-examples",
    "description": "Examples for RabbitMQ",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Thibaud BARDIN",
            "email": "thibaud.bardin@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.3",
        "incenteev/composer-parameter-handler": "~2.0",
        "symfony/yaml": "~2.5",
        "videlalvaro/php-amqplib": "~2.4"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ]
    },
    "extra": {
        "incenteev-parameters": {
            "file": "parameters.yml"
        }
    }
}